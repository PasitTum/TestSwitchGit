<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" type="image/x-icon" href="~/Content/Images/favicon.ico">
    <title>@ViewBag.Title</title>

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title></title>
    <!-- Bootstrap CSS CDN -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <!-- Our Custom CSS -->

    <link rel="stylesheet" href="~/Content/Site.css?id=20200504">

    @*<link rel="stylesheet" href="~/Content/classy-nav.css"> ไม่รู้ใช้ทำไร *@
    <link rel="stylesheet" href="~/Content/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Content/animate.css">
    @* <link rel="stylesheet" href="~/Content/magnific-popup.css">*@

    <!-- Font Awesome JS -->
    <link href="~/vender/fontawesome-5.12.1/css/all.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <!-- Popper.JS -->
    <script src="~/Scripts/umd/popper.js"></script>
    <!-- Angular JS -->
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/angular-animate.min.js"></script>
    <script src="~/Scripts/angular-touch.min.js"></script>
    <!-- Bootstrap JS -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/ui-bootstrap.js"></script>
    <script src="~/Scripts/ui-bootstrap-tpls.js"></script>
</head>
<body>
    <div class="wrapper">

        <!-- Page Content  -->
        <div id="content">
            <a href="@Url.Action("Index","Home")">
                <div class="site-banner-main">

                </div>
            </a>

            <div class="container body-content">
                @RenderBody()
            </div>
        </div>
    </div>

    @Html.Partial("_Footer")

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script>
        var WWW_ROOT = "@Url.Content("~")";
        var API_ROOT = "@OCS.Register.Web.Helpers.SysParameterHelper.ApiUrlClientSide";
    </script>
    <script src="~/Scripts/csp-angular/ng-csp-module.js"></script>
    <script src="~/Scripts/csp-angular/ng-lookup-service.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>

                                        class="form-control col-12 col-md-9" required>
                                    <option value="">กรุณาเลือกจังหวัด</option>
                                    <option ng-repeat="province in lstProvince" value="{{province.id}}">{{province.text}}</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="" class="control-label col-12 col-md-2 col-form-label required ml-3">คำนำหน้า</label>

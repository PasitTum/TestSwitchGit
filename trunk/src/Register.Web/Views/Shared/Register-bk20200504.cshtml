                        <div class="input-group col-12 col-md-8">
                            <div class="custom-control custom-radio custom-control-inline mt-2 ml-2 mr-0" ng-repeat="prefix in prefixes">
                                <input class="custom-control-input col-form-label" type="radio"
                                       ng-model="model.PrefixID"
                                       id="prefix_{{prefix.id}}"
                                       value="{{prefix.id}}" required>
                                <label class="custom-control-label pointer" onclick="" for="prefix_{{prefix.id}}">{{prefix.text}}</label>
                            </div>
                            <input type="hidden" id="hdPrefixID" value="{{model.PrefixID = '@Model.PrefixID'}}" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-12 col-md-6">
                            <label for="" class="control-label col-12 col-md-12 col-form-label required">ชื่อ</label>

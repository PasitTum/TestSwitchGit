                                <option value="">กรุณาเลือกปริญญา/ประกาศนียบัตร</option>
                                <option ng-repeat="degree in lstDegree" value="{{degree.id}}">{{degree.text}}</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="" class="control-label col-12 col-md-3 col-form-label required ml-3">วิชาเอก</label>
                        <div class="input-group col-12 col-md-6" ng-class="{ 'validation-error': register.majorName.$error.required  && register.majorName.$dirty }">
                            <input type="text" class="form-control ml-3 ml-md-0" id="majorName" name="majorName"
                                   ng-model="model.MajorName" autocomplete="off" placeholder="วิชาเอก" required />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="" class="control-label col-12 col-md-2 col-form-label required ml-3">สถานะการศึกษา</label>
                        @* แก้ validate ด้วย *@
                        <div class="input-group col-12 col-md-8">
                            <div class="custom-control custom-radio custom-control-inline mt-2 ml-2 mr-0">
                                <input class="custom-control-input col-form-label" type="radio"
                                       ng-model="model.EducationStatus"
                                       id="educationStatus_G"
                                       value="G" required>
                                <label class="custom-control-label pointer" onclick="" for="educationStatus_G">จบการศึกษา</label>
                            </div>
                            <div class="custom-control custom-radio custom-control-inline mt-2 ml-5 mr-0 pointer">
                                <input class="custom-control-input col-form-label" type="radio"
                                       ng-model="model.EducationStatus"
                                       id="educationStatus_S"
                                       value="S" required>
                                <label class="custom-control-label pointer" onclick="" for="educationStatus_S">กำลังศึกษา</label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="" class="control-label col-12 col-md-3 col-form-label required ml-3">วัน-เดือน-ปี ที่จบการศึกษา</label>
                        <div class="input-group col-6 col-md-6 pointer" style="max-width: 200px;"

                        <div class="input-group col-12 col-md-6" ng-class="{ 'validation-error': register.gpa.$error.required  && register.gpa.$dirty }">
                            <input type="text" class="form-control ml-3 ml-md-0" id="gpa" name="gpa"
                                   ng-model="model.GPA" autocomplete="off" placeholder="เกรดเฉลี่ยสะสม" required />
                        </div>
                    </div>
                    <div class="form-group row" ng-if="model.EducationStatus == 'S'">
